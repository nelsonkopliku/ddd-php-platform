<?xml version="1.0"?>
<!-- autoloader="../../../app/vendor/autoload.php" needs to be moved to module autoloader  -->
<psalm
    autoloader="../../../app/vendor/autoload.php"
    totallyTyped="false"
    errorLevel="3"
    resolveFromConfigFile="true"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://getpsalm.org/schema/config"
    xsi:schemaLocation="https://getpsalm.org/schema/config ../../../app/vendor/vimeo/psalm/config.xsd"
>
    <projectFiles>
        <directory name="src" />
        <ignoreFiles>
            <directory name="vendor" />
        </ignoreFiles>
    </projectFiles>
</psalm>
